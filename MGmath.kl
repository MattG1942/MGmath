// Trig functions


operator multiSin<<<index>>>(Float32 a[],io Float32 returnArray[]) {
  returnArray[index] = sin(a[index]);
}

operator multiCos<<<index>>>(Float32 a[],io Float32 returnArray[]) {
  returnArray[index] = cos(a[index]);
}